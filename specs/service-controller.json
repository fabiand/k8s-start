{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "controller",
    "labels": {
      "app": "controller-service"
    }
  },
  "spec": {
    "selector": {
      "app": "controller"
    },
    "ports": [
      {
        "name": "controller-rest",
        "port": 8080,
        "targetPort":"controller-rest"
      },
      {
        "name": "etcd-rest",
        "port": 4001,
        "targetPort":"etcd-rest"
      }
    ]
  }

}
