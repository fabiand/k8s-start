{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "ovirt-engine"
  },
  "spec": {
    "selector": {
      "app": "ovirt-engine"
    },
    "ports": [
      {
        "name": "engine",
        "protocol": "TCP",
        "port": 80,
        "targetPort": "engine"
      },
      {
        "name": "engine-tls",
        "protocol": "TCP",
        "port": 8444,
        "targetPort": "engine-tls"
      },
      {
        "name": "frontend",
        "protocol": "TCP",
        "port": 443,
        "targetPort": "frontend"
      }

    ],
    "externalIPs": [
        "YOUR_IP_HERE"
    ]
  }
}
